{
  "name": "mcp-parasut",
  "version": "1.0.0",
  "description": "Unofficial TypeScript SDK and MCP Server for Para\u015f\u00fct API - Turkish accounting and e-invoicing",
  "scripts": {
    "build": "pnpm -r build",
    "build:sdk": "pnpm --filter @yigitkonur/parasut-node-sdk build",
    "build:mcp": "pnpm --filter @yigitkonur/parasut-mcp-server build",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r --if-present lint",
    "test": "pnpm -r --if-present test",
    "clean": "rm -rf packages/*/dist packages/*/node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yigitkonur/mcp-parasut.git"
  },
  "keywords": [
    "parasut",
    "para\u015f\u00fct",
    "sdk",
    "mcp",
    "typescript",
    "e-fatura",
    "e-invoice",
    "turkey",
    "accounting",
    "api"
  ],
  "author": "Yigit Konur <yigit@konur.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yigitkonur/mcp-parasut/issues"
  },
  "homepage": "https://github.com/yigitkonur/mcp-parasut#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
